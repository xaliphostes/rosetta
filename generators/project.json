{
    "project": {
        "name": "myproject",
        "version": "1.0.0",
        "description": "My C++ library with auto-generated bindings",
        "author": "Your Name",
        "license": "MIT"
    },
    "rosetta": {
        "registration_header": "src/bindings/registration.h",
        "registration_namespace": "myproject_rosetta",
        "registration_function": "register_classes",
        "types_namespace": "myproject"
    },
    "includes": {
        "directories": [
            "include",
            "src",
            "../external/rosetta/include"
        ],
        "headers": [
            "myproject/core/Types.h",
            "myproject/core/Model.h",
            "myproject/core/Solver.h",
            "myproject/math/Vector3.h",
            "myproject/math/Matrix33.h"
        ],
        "libraries": [
            "myproject_core",
            "rosetta"
        ]
    },
    "output": {
        "base_dir": "./generated"
    },
    "targets": {
        "python": {
            "enabled": true,
            "output_dir": "",
            "extra_sources": [],
            "extra_libs": []
        },
        "wasm": {
            "enabled": true,
            "output_dir": "",
            "extra_sources": [],
            "extra_libs": []
        },
        "javascript": {
            "enabled": false,
            "output_dir": "",
            "extra_sources": [],
            "extra_libs": []
        },
        "rest": {
            "enabled": false,
            "output_dir": "",
            "extra_sources": [],
            "extra_libs": []
        }
    },
    "options": {
        "generate_stubs": true,
        "generate_typescript": true,
        "generate_readme": true,
        "generate_cmake": true
    },
    "advanced": {
        "numpy_types": [
            "Vector3",
            "Matrix33",
            "std::vector<double>",
            "std::vector<float>",
            "std::vector<int>"
        ],
        "skip_classes": [],
        "skip_methods": []
    }
}