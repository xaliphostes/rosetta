{
    "variables": {
        "PROJECT_ROOT": "../..",
        "ROSETTA_ROOT": "../../../../",
        "BUILD_DIR": "${PROJECT_ROOT}/build"
    },
    "advanced": {
        "numpy_types": [
            "std::vector<double>"
        ],
        "skip_classes": [],
        "skip_methods": []
    },
    "includes": {
        "directories": [
            "${PROJECT_ROOT}",
            "${ROSETTA_ROOT}/include/",
            "../../.."
        ],
        "headers": [
            "third/common.h",
            "third/Model.h",
            "third/Point.h",
            "third/Surface.h",
            "third/Triangle.h"
        ],
        "libraries": [
        ]
    },
    "options": {
        "generate_cmake": true,
        "generate_readme": true,
        "generate_example": true,
        "generate_stubs": true,
        "generate_typescript": true
    },
    "output": {
        "base_dir": "./build/generated"
    },
    "project": {
        "author": "xaliphostes",
        "description": "Geometry C++ library with auto-generated bindings",
        "license": "MIT",
        "name": "geometry",
        "version": "1.0.0"
    },
    "rosetta": {
        "registration_function": "register_rosetta_classes",
        "registration_header": "registration.h",
        "registration_namespace": "",
        "types_namespace": "myproject"
    },
    "targets": {
        "javascript": {
            "enabled": true,
            "extra_libs": [],
            "extra_sources": [],
            "output_dir": ""
        },
        "python": {
            "enabled": true,
            "extra_libs": [],
            "extra_sources": [],
            "output_dir": ""
        },
        "rest": {
            "enabled": true,
            "extra_libs": [],
            "extra_sources": [],
            "output_dir": ""
        },
        "wasm": {
            "enabled": true,
            "extra_libs": [],
            "extra_sources": [],
            "output_dir": ""
        }
    }
}